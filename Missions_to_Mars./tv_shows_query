CREATE TABLE amazon (
	id int primary key,
	title varchar (40),
	year int,
	seasons_available int,
	language TEXT,
	genre TEXT,
	imdb_rating TEXT,
	age_of_viewrs TEXT
),

CREATE TABLE netflix_show(
	id serial primary key,
	title TEXT,
	year INT,
	age_of_viewer TEXT,
	imdb_rating TEXT,
	netflix INT
);

CREATE TABLE all_tv (
	id INT PRIMARY KEY,
	title TEXT,
	year INT,
	age_of_viewers TEXT,
	imdb_rating TEXT,
	rotten_tomatoes	TEXT,
	netflix INT,
	hulu INT,
	prime_video INT,
	disney INT
);

CREATE TABLE netflix (
	id INT PRIMARY KEY,
	type TEXT,
	title TEXT,
	director TEXT,
	actors TEXT,
	country TEXT,
	year INT,
	age_of_viewers TEXT,
	duration TEXT,
	genre TEXT,
	description TEXT
);
